{
  "expo": {
    "name": "Thermalib Expo",
    "slug": "thermalib-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/thermalib-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/thermalib-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.thermalibexpo.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/thermalib-1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.thermalibexpo.example"
    },
    "web": {
      "favicon": "./assets/thermalib-48.png"
    },
    "plugins": [
      "../app.plugin.js",
      ["expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
      }]
    ]
  }
}