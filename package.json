{
  "name": "@mobione/thermalib-expo",
  "version": "0.1.9",
  "description": "ETI Bluetherm LE Protocol 1.1 integration",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "adb:connect": "adb reverse tcp:8081 tcp:5555",
    "analyze": "size-limit --why",
    "android:build": "npm run prepare && npm run prepub && cd example && npm i && npm run prebuild:debug && npm run android --debug",
    "android": "expo run:android",
    "build:watch": "expo-module build --watch",
    "build": "expo-module build",
    "changelog:generate": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "changelog:release": "npm run changelog:generate && git add CHANGELOG.md && git commit -m 'chore: update changelog' || echo 'No changelog changes'",
    "clean:jest": "rm -rf ./.jest",
    "clean": "npm run clean:jest && expo-module clean",
    "ios:build": "npm run prepare && npm run prepub && npm run pods && cd example && npm run prebuild:debug && npm run ios --debug --device",
    "ios": "expo run:ios --device",
    "lint:ci": "npm run lint:es",
    "lint:errors": "npm run ts & npm run lint:es --fix --quiet",
    "lint:es": "expo-module lint",
    "lint": "npm run ts && npm run lint:es --fix",
    "open:android": "open -a \"Android Studio\" example/android",
    "open:ios": "xed example/ios",
    "pods": "npx pod-install",
    "prep": "npm run prepare && npm run prepub",
    "prepare": "expo-module prepare && husky",
    "prepub": "npm run prepublishOnly",
    "prepublishOnly": "expo-module prepublishOnly",
    "test:cov": "npm run test -u --coverage --forceExit",
    "test:dev": "jest -u --maxWorkers=25% --forceExit",
    "test:watch": "jest -u --watchAll --maxWorkers=25%",
    "test": "expo-module test",
    "ts": "tsc -p ./tsconfig.json",
    "verify": "npm run ts && npm run lint && npm run test:dev && npm run prepub && npm run prepare",
    "version:next": "npm run release:patch"
  },
  "keywords": [
    "react-native",
    "expo",
    "thermalib-expo",
    "ThermalibExpo",
    "bluetherm",
    "eti",
    "thermapen"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MobiDevel/thermalib-expo.git"
  },
  "bugs": {
    "url": "https://github.com/MobiDevel/thermalib-expo/issues"
  },
  "author": "Thomas HagstroÌˆm <thomas.hagstrom@mobione.com> (https://github.com/thomashagstrom)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/MobiDevel/thermalib-expo#readme",
  "devDependencies": {
    "@types/react": "~19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-native": "^5.0.0",
    "expo": "^54.0.27",
    "expo-location": "~19.0.8",
    "expo-module-scripts": "^5.0.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "expo": "*",
    "expo-location": "*",
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "jest-expo",
    "cacheDirectory": ".jest/cache",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "dependencies": {
  }
}
